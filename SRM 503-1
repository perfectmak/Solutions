#include <iostream>

using namespace std;


class ToastXRaspberry
{
public:
    int apply(int upper_limit, int layer_count)
    {
        int answer = layer_count/upper_limit;        
        return answer+1;
    }
};

/*
 * 
 */
int main(int argc, char** argv) 
{
    ToastXRaspberry r;
    
    cout << r.apply(73, 265) << endl;
    return 0;
}

